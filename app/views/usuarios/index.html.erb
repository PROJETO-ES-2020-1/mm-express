<body class="page">
<p id="notice"><%= notice %></p>

<h1>Usuarios</h1>

<table cellpadding="0" border="0" align="center" cellspacing="0">
  <thead class="head_tabela">
    <tr>
      <th>Nome</th>
      <th>Email</th>
      <th>Telefone</th>
      <th>Cpf</th>
      <th>Numero residencia</th>
      <th>Bloco residencia</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuarios.each do |usuario| %>
      <tr>
        <td><%= usuario.nome %></td>
        <td><%= usuario.email %></td>
        <td><%= usuario.telefone %></td>
        <td><%= usuario.cpf %></td>
        <td><%= usuario.numero_residencia %></td>
        <td><%= usuario.bloco_residencia %></td>
        <td><%= link_to "Visualizar", usuario %></td>
        <td><%= link_to "Editar", edit_usuario_path(usuario) %></td>
        <td><%= link_to "Excluir", usuario, method: :delete, data: { confirm: "Are you sure?" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to "Cadastrar novo usuario", new_usuario_path %>
