<body class="page">
<p id="notice"><%= notice %></p>

<h1>Sobreaviso De Entregas</h1>

<table cellpadding="0" border="0" align="center" cellspacing="0">
  <thead class="head_tabela">
    <tr>
      <th>Data prevista entrega</th>
      <th>Urgencia de repasse</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sobreaviso_de_entregas.each do |sobreaviso_de_entrega| %>
      <tr>
        <td><%= sobreaviso_de_entrega.data_prevista_entrega %></td>
        <td><%= sobreaviso_de_entrega.urgencia_de_repasse %></td>
        <td><%= link_to "Visualizar", sobreaviso_de_entrega %></td>
        <td><%= link_to "Editar", edit_sobreaviso_de_entrega_path(sobreaviso_de_entrega) %></td>
        <td><%= link_to "Deletar", sobreaviso_de_entrega, method: :delete, data: { confirm: "Are you sure?" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to "Cadastrar sobreaviso de entrega", new_sobreaviso_de_entrega_path %>
