<p id="notice"><%= notice %></p>

<h1>Sobreaviso De Entregas</h1>

<table>
  <thead>
    <tr>
      <th>Data prevista entrega</th>
      <th>Urgencia de repasse</th>
      <th>Usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sobreaviso_de_entregas.each do |sobreaviso_de_entrega| %>
      <tr>
        <td><%= sobreaviso_de_entrega.data_prevista_entrega %></td>
        <td><%= sobreaviso_de_entrega.urgencia_de_repasse %></td>
        <td><%= sobreaviso_de_entrega.usuario_id %></td>
        <td><%= link_to 'Show', sobreaviso_de_entrega %></td>
        <td><%= link_to 'Edit', edit_sobreaviso_de_entrega_path(sobreaviso_de_entrega) %></td>
        <td><%= link_to 'Destroy', sobreaviso_de_entrega, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sobreaviso De Entrega', new_sobreaviso_de_entrega_path %>
